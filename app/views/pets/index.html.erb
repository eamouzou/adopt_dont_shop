<h2>All Pets</h2>
  <%@pets.each do |pet|%>
  <img src="<%=  pet.image %>" alt="<%= pet.image %>" style="width:200px">
  <h4>Name: <%= pet.name %></h4>
  <h4>Approximate Age: <%= pet.approximate_age %></h4>
  <h4>Sex: <%= pet.sex %></h4>
  <h4>Shelter Name: <%= pet.shelter_name %></h4>
  </p>
  <% end %>
