<h1> All Shelters </h1>

  <% @shelters.each do |shelter| %>
    <h2><a href="/shelters/<%= shelter.id %>"><%= shelter.name %></a>
      <form action="/shelters/<%= shelter.id %>/edit">
        <button type="submit">Edit <%= shelter.name %></button>
      </form>
    </h2>

  <% end %>
<a href="/shelters/new">Create New Shelter</a>
